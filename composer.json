{
    "name": "sgu-infocom-official/date-picker-masked-widget",
    "type": "yii2-extension",
    "description": "Bootstrap DatePicker widget for Yii2 along with the Yii input mask functionality.",
    "keywords": [
        "datepicker",
        "yii",
        "yii2",
        "yii 2",
        "extension",
        "widget",
        "inputmask",
        "masked input"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "sitnikov.t.v",
            "email": "t.sitnikov@infocom-s.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.13",
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "bower-asset/bootstrap-datepicker": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "sguinfocom\\DatePickerMaskedWidget\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower-asset"
        }
    }
}
